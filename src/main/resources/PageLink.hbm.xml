<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="jwm.ir.domain">
	<class name="PageLink" table="pagelinks">
		<id name="id" column="pageLinkId">
			<generator class="native"/>
		</id>
		<many-to-one name="page" column="pageId" class="Page" not-null="true"/>
		<many-to-one name="destinationPage" column="destPageId" class="Page" not-null="true"/>
	</class>
</hibernate-mapping>
