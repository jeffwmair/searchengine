<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="jwm.ir.domain">
	<class name="Page" table="Pages">
		<id name="id" column="pageId">
			<generator class="native"/>
		</id>
		<property name="domainId" />
		<property name="title" />
		<property name="description" />
		<property name="url" />
		<property name="verified" />
		<property name="failCount" column="fail_count" />
		<property name="pageRank" />
		<property name="lastCrawl" column="last_crawl" />
	</class>
</hibernate-mapping>
