<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="jwm.ir.domain">
	<class name="Domain" table="domains">
		<id name="id" column="domainId">
			<generator class="native"/>
		</id>
		<property name="domain" />
		<property name="status" />
		<property name="crawlerId" />
		<property name="lastCrawl" column="last_crawl" />
		<property name="locked" />
		<property name="totalCrawls" column="total_crawls" />
	</class>
</hibernate-mapping>
